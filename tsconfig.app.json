{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": ["src/**/*.d.ts", "src/**/*", "src/**/*.vue"],
    "exclude": ["src/**/__tests__/*", "node_modules", "dist", "wailsjs", "vite.config.ts"],
    "compilerOptions": {
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",

        "paths": {
            "@/*": ["./src/*"]
        }
    }
}
